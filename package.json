{
  "name": "codehskarel-tester",
  "version": "1.0.1",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "test-codehs-submission": "lib/testSubmission.js"
  },
  "scripts": {
    "test": "ts-mocha test/*.test.ts",
    "watch": "ts-mocha --reporter min -w test/*test.ts --watch-files test/*test.ts,src/**/*.ts",
    "build": "tsc && chmod u+x lib/testSubmission.js",
    "coverage": "nyc yarn test && rm -r .nyc_output",
    "make-badges": "yarn coverage && istanbul-badges-readme"
  },
  "nyc": {
    "reporter": [
      "json-summary"
    ]
  },
  "files": [
    "src",
    "lib"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/rewire": "^2.5.28",
    "commander": "^5.0.0",
    "dt-types": "^1.0.2",
    "jskarel": "^1.0.1",
    "rewire": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.8",
    "chai": "^4.2.0",
    "istanbul-badges-readme": "^1.8.4",
    "mocha": "^7.1.1",
    "nyc": "^15.1.0",
    "ts-mocha": "^7.0.0",
    "typescript": "^3.8.3"
  }
}
